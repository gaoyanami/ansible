gpgcheck=0
gpgkey={{ docker_repo_gpgkey }}

[docker-ce-stable-debuginfo]
name=Docker CE Stable - Debuginfo $basearch
baseurl={{ docker_repo_baseurl }}/centos/7/debug-$basearch/stable
